package geometry;

import utility.Ray;

public class Intersection
{
	public double			t;
	public double			tmp;
	public GeometricObject	obj;
	
	double closestHit(GeometricObject[] objArr, Ray ray)
	{
		t = 0xffffff;
		tmp = 0.0;
		for (int i = 0; i < objArr.length; i++)
		{
			tmp = objArr[i].hit(ray);
			if (tmp < t && tmp > 0.0)
		}
		return (t);
	}
}
